{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "config/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "extensions/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "middlewares/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "plugins/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
